alias ls ='ls -h --color=auto'
alias ll ='ls -alh --color-auto'
alias llt ='ls -alt'
alias findn='find . -name'
alias grep='grep -i -n'
alias grepcpp='grep -i -n --include=\*.cpp --include=\*.hpp --include=\*.c --include=\*.h --include=\*.cc'
alias kppath='echo `pwd` > ~/.lastpath'
alias cdlast='cd `cat ~/.lastpath`'
